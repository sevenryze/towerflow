{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm start",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/bin.js",
      "args": ["start"],
      "cwd": "${workspaceFolder}/test-app",
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "<node_internals>/**/*.js",
        "webpack/bootstrap"
      ],
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Init web-app",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/bin.js",
      "args": ["init", "test-web-app", "--template", "web-app", "--bypass-npm"],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "console": "integratedTerminal",
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Init web-lib",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/bin.js",
      "args": ["init", "test-web-lib", "--template", "web-lib", "--bypass-npm"],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "console": "integratedTerminal",
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Init node-app",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/bin.js",
      "args": [
        "init",
        "test-node-app",
        "--template",
        "node-app",
        "--bypass-npm"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "console": "integratedTerminal",
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Init node-lib",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/bin.js",
      "args": [
        "init",
        "test-node-lib",
        "--template",
        "node-lib",
        "--bypass-npm"
      ],
      "cwd": "${workspaceFolder}",
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "console": "integratedTerminal",
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "npm build",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/bin/index.js",
      "args": ["build"],
      "cwd": "${workspaceFolder}/test-app",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "console": "integratedTerminal",
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "npm config-files",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/bin/index.js",
      "args": ["config-files", "--remove"],
      "cwd": "${workspaceFolder}/test-app",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "console": "integratedTerminal",
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Test test-app start",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--preserve-symlinks"],
      "program": "${cwd}/node_modules/webpack-cli/bin/cli.js",
      "args": ["run", "start"],
      "cwd": "${workspaceFolder}/test-app",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Random test",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--preserve-symlinks"],
      "program": "${workspaceFolder}/node_modules/webpack-cli/bin/cli.js",
      "args": ["--profile", "--json > stats.json"],
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "name": "Random test 2",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--preserve-symlinks"],
      "program": "${workspaceFolder}/test-node/dist/index.js",
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "autoAttachChildProcesses": true,
      "smartStep": true,
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "<node_internals>/**/*.js",
        "webpack/bootstrap"
      ],
      "outputCapture": "std", // 解决显示到process.stdout.write的内容不显示的问题
      "env": {
        "DEBUG": "towerflow:*"
      }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost 8080",
      "url": "http://localhost:8080",
      "smartStep": true,
      "skipFiles": ["**/node_modules/**"]
    }
  ]
}
